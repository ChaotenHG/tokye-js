{
  "name": "@tokye/ui",
  "packageManager": "yarn@4.5.0",
  "description": "Tokye",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "./dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "autoprefixer": "^10",
    "daisyui": "^3",
    "postcss": "^8",
    "tailwindcss": "^3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "dependencies": {
    "@tokye/sdk": "workspace:^"
  }
}
